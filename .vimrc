set incsearch
set hlsearch
set number
set numberwidth=4
set relativenumber
set tabstop=4
set softtabstop=4
set expandtab
set smartindent
set shiftwidth=4
set splitbelow
set splitright
set hidden
set scrolloff=3
set scroll=5
set nowrap
set wildmode=list,full
set background=dark
syntax on
colorscheme habamax
hi! Normal ctermbg=NONE guibg=NONE

nnoremap <C-s> :w<CR>
inoremap <Del> <C-w>
nnoremap W $

